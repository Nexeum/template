{
    "manifestVersion": 1.0,
    "id": "Performance",
    "publisher": "Performance",
    "version": "1.0.0",
    "public": true,
    "name": "Checklist Performance",
    "description": "Extension to assess the need for performance testing",
    "categories": [
        "Azure Boards"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "img.jpg"    
    },
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "scopes": [
        "vso.work"
    ],
    "files": [
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        }
    ],
    "tags": [
        "Work Items",
        "Extensions",
        "Work Item Control"
    ]
}
